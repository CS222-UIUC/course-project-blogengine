"\n                            className=\"form-control\"\n                            id=\"exampleFormControlTextarea1\"\n                            rows=\"20\"\n                            placeholder=\"Let the ink flow!\"\n                            onChange={handleChange}\n                            value={input.content}\n                        ></textarea>\n                    </div>\n                    <div className=\"col-sm-6\">\n                        <div dangerouslySetInnerHTML={renderText(input.content)}></div>\n                    </div>\n                </div>\n            </div>\n\n            <button className=\"btn btn-primary\">Post</button>\n        </form>\n    </div>\n  );\n}\n","/Users/Amy1/programFiles/cs222/course-project-blogengine/blog/react-app/src/Navbar.js",[],[],"/Users/Amy1/programFiles/cs222/course-project-blogengine/blog/react-app/src/Posts.js",[],[],{"ruleId":"47","severity":1,"message":"48","line":9,"column":27,"nodeType":"49","messageId":"50","endLine":9,"endColumn":33},{"ruleId":"51","severity":1,"message":"52","line":24,"column":13,"nodeType":"53","endLine":24,"endColumn":124},{"ruleId":"47","severity":1,"message":"54","line":124,"column":9,"nodeType":"49","messageId":"50","endLine":124,"endColumn":20},"no-unused-vars","'Router' is defined but never used.","Identifier","unusedVar","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","'handleClick' is assigned a value but never used."]